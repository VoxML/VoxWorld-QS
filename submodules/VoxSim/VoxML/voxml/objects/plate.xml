<?xml version="1.0" encoding="us-ascii"?>
<VoxML xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <Entity Type="Object" />
  <Lex>
    <Pred>plate</Pred>
    <Type>physobj*artifact</Type>
  </Lex>
  <Type>
    <Head>sheet[1]</Head>
    <Components>
      <Component Value="base" />
      <Component Value="top" />
    </Components>
    <Concavity>Concave</Concavity>
    <RotatSym>Y</RotatSym>
    <ReflSym>XY,YZ</ReflSym>
  </Type>
  <Habitat>
      <Intrinsic>
          <Intr Name="UP[2]" Value="align(Y,E_y)" />
          <Intr Name="TOP[2]" Value="top(+Y)" />
      </Intrinsic>
    <Extrinsic />
  </Habitat>
  <Afford_Str>
    <Affordances>
      <Affordance Formula="H-&gt;[put(x, on([1]))]support([1], x)" />
      <Affordance Formula="H[2]-&gt;[put(x, in([1]))]contain([1], x)" />
      <Affordance Formula="H-&gt;[grasp(x, [1])]hold(x, [1])" />
      <Affordance Formula="H-&gt;[lift(x, [1])]hold(x, [1])" />
      <Affordance Formula="H-&gt;[slide([1])]" />
    </Affordances>
  </Afford_Str>
  <Embodiment>
    <Scale>&lt;agent</Scale>
    <Movable>true</Movable>
  </Embodiment>
  <Attributes>
      <Attrs>
          <Attr Value="white" />
      </Attrs>
  </Attributes>
</VoxML>
